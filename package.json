{
    "name": "ieaseMusic",
    "version": "1.0.0",
    "main": "./dist/main.js",
    "author": "daihy",
    "license": "MIT",
    "private": false,
    "scripts": {
        "start-renderer-dev": "webpack serve --config webpack/webpack.renderer.dev.config.js",
        "start-main-dev": "webpack --config webpack/webpack.main.config.js && electron ./dist/main.js",
        "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
        "build-main": "webpack --config webpack/webpack.main.prod.config.js",
        "build-renderer": "webpack --config webpack/webpack.renderer.prod.config.js",
        "build-all": "concurrently \"npm:build-main\" \"npm:build-renderer\"",
        "build-analyzer": "cross-env analyzer=1 npm run build-renderer",
        "clean": "rimraf dist && rimraf build",
        "build": "npm run clean && npm run build-all && electron-builder",
        "lint": "eslint --ext .ts,.tsx src/**",
        "lint:fix": "prettier --write ./src/**/**/**/*"
    },
    "build": {
        "productName": "ieaseMusic",
        "appId": "com.github.daihy8759",
        "mac": {
            "category": "com.github.daihy8759"
        },
        "directories": {
            "output": "release",
            "buildResources": "resources"
        },
        "files": [
            "dist/"
        ],
        "win": {
            "target": "nsis"
        }
    },
    "dependencies": {
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.11.2",
        "NeteaseCloudMusicApi": "^3.47.5",
        "axios": "^0.21.0",
        "classnames": "^2.2.6",
        "date-fns": "^2.16.1",
        "debug": "^4.3.1",
        "delegate": "^3.2.0",
        "electron-store": "^6.0.1",
        "electron-window-state": "^5.0.3",
        "libphonenumber-js": "^1.9.6",
        "lodash.throttle": "^4.1.1",
        "mobx": "^6.0.4",
        "mobx-react-lite": "^3.1.6",
        "qrcode": "^1.4.4",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "react-transition-group": "^4.4.1",
        "react-use": "^15.3.4",
        "tiny-pinyin": "^1.3.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
        "@types/classnames": "^2.2.11",
        "@types/electron-devtools-installer": "^2.2.0",
        "@types/lodash.throttle": "^4.1.6",
        "@types/qrcode": "^1.3.5",
        "@types/qs": "^6.9.5",
        "@types/random-useragent": "^0.3.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.6",
        "@types/react-transition-group": "^4.4.0",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-import": "^1.13.3",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "electron": "^11.1.0",
        "electron-builder": "^22.9.1",
        "electron-devtools-installer": "^3.1.1",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.4.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.0",
        "image-webpack-loader": "^7.0.1",
        "less": "^4.0.0",
        "less-loader": "^7.1.0",
        "mini-css-extract-plugin": "^1.3.3",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "prettier": "^2.2.1",
        "react-hot-loader": "^4.13.0",
        "react-refresh": "0.9.0-rc.3",
        "rimraf": "^3.0.2",
        "style-loader": "^2.0.0",
        "typescript": "^4.1.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.11.0",
        "webpack-bundle-analyzer": "^4.3.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.7.2"
    }
}
