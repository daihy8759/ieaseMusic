{
    "presets": ["@babel/preset-react", "@babel/typescript"],
    "plugins": [
        [
            "babel-plugin-import",
            {
                "libraryName": "@material-ui/core",
                "libraryDirectory": "esm",
                "camel2DashComponentName": false
            },
            "core"
        ],
        [
            "babel-plugin-import",
            {
                "libraryName": "@material-ui/icons",
                "libraryDirectory": "esm",
                "camel2DashComponentName": false
            },
            "icons"
        ],
        "@babel/plugin-syntax-dynamic-import"
    ],
    "comments": true
}
